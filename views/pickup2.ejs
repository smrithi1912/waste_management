	<div class="container-contact100">
		<div class="wrap-contact100">
			<form class="contact100-form validate-form"  action="/pickup" method="POST">
				<span class="contact100-form-title">
					Send Us Your Waste
				</span>


				<div class="wrap-input100 validate-input" data-validate="Name is required">
					<label class="label-input100" for="name">Full name</label>
					<input id="name" class="input100" type="text" name="name" placeholder="Enter your name...">
					<span class="focus-input100"></span>
				</div>


				<div class="wrap-input100 validate-input" data-validate="Number is required">
					<label class="label-input100" for="number">Phone Number</label>
					<input id="number" class="input100" type="tel" name="phonenumber" placeholder="Format: 123-45-678" pattern="[0-9]{3}-[0-9]{2}-[0-9]{3}">
					<span class="focus-input100"></span>
				</div>

				<div class="wrap-input100 validate-input" data-validate="Field is required">
					<label class="label-input100" for="number">Aadhar Card Number</label>
